<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>

   <extension
         point="org.eclipse.ui.importWizards">
      <category
            id="com.cotechnoe.ontocase.eli.exporter.importWizards.sampleCategory"
            name="G-OWL File Import">
      </category>
      <wizard
            id="com.cotechnoe.ontocase.eli.exporter.importWizards.ImportWizard"
            name="Import File"
            class="com.cotechnoe.ontocase.eli.exporter.importWizards.ImportWizard"
            category="com.cotechnoe.ontocase.eli.exporter.importWizards.sampleCategory"
            icon="icons/sample.png">
         <description>
            Import a file from the local file system into the workspace.
         </description>
      </wizard>
   </extension>
   <extension
         point="org.eclipse.ui.exportWizards">
      <category
            id="com.ontocase..eli.exporter.exportWizards.exportWizardCategory"
            name="G-OWL File Export">
      </category>
      <wizard
            category="com.ontocase..eli.exporter.exportWizards.exportWizardCategory"
            class="com.cotechnoe.ontocase.eli.exporter.ExportWizards.exportWizards"
            id="com.cotechnoe.ontocase.eli.exporter.exportWizards.exportWizard"
            name="exportGowlFile">
         <description>
            export a G-OWL file
         </description>
      </wizard>
   </extension>
</plugin>
